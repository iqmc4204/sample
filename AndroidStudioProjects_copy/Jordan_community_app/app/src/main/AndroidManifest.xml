<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seongyungo.jordan_community_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="Splash_Activity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="Login_Activity"
                    android:label="@layout/login_activity"
            />
        
        <activity android:name="Tab_Activity"
            android:label="@layout/tab_activity"/>
        
        <activity android:name="Main_Activity"
            android:label="@layout/main_activity"/>




        <activity android:name="My_Profile_Activity"
            android:label="@layout/my_profile_activity"/>

        <activity android:name="Community_Activity"
            android:label="@layout/community_activity"/>

        <activity android:name="Info_App_Activity"
            android:label="@layout/info_app_activity"/>
        
    </application>

</manifest>
